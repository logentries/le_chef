{
  "reporter": {
    "user_key": "<%= @user_key %>",
    "interval": <%= @interval %>,
    "token": "not used",
    "autoupdate": <%= @autoupdate %>,
    "update_datahub": <%= @update_datahub %>
  },
  "backend": {
    "local_path": "<%= @local_path %>",
    "amazon_s3_secret_key": "<%= @amazon_s3_secret_key %>",
    "amazon_s3_account_id": "<%= @amazon_s3_account_id %>",
    "amazon_s3_bucket": "<%= @amazon_s3_bucket %>",
    "amazon_s3_encryption_key_file": "<%= @amazon_s3_encryption_key_file %>"
  },
 "logging": {
    "default": {
      "logInterval": <%= @logging['default']['logInterval'] %>,
      "logLocally": <%= @logging['default']['logLocally'] %>,
      "sendToLogentries": <%= @logging['default']['sendToLogentries'] %>
    },
    "heartbeat": {
      "logInterval": <%= @logging['heartbeat']['logInterval'] %>,
      "logLocally": <%= @logging['heartbeat']['logLocally'] %>,
      "sendToLogentries": <%= @logging['heartbeat']['sendToLogentries'] %>
    },
    "system-stats": {
      "logInterval": <%= @logging['system-stats']['logInterval'] %>,
      "logLocally": <%= @logging['system-stats']['logLocally'] %>,
      "sendToLogentries": <%= @logging['system-stats']['sendToLogentries'] %>
    },
  },
  "rules": <%= @rules %>,
  "filters": <%= @filters %>
}
